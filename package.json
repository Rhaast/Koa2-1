  },
  "homepage": "https://github.com/szy0syz/Koa2#readme",
  "dependencies": {
    "bcrypt": "^2.0.1",
    "co": "^4.6.0",
    "glob": "^7.1.2",
    "koa": "^2.5.1",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.2",
    "koa-views": "^6.1.4",
    "mongoose": "^5.1.1",
    "nanoid": "^1.0.2",
    "node-fetch": "^2.1.2",
    "parcel-bundler": "^1.8.1",
