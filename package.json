{
  "name": "koa2",
  "version": "1.0.0",
  "description": "koa2 practice",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon -w src --exec \"babel-node src --presets env\"",
    "build": "rimraf dist && babel src -s -D -d dist --presets env",
    "production": "node dist",
    "pracelBuild": "rimraf parcel/dist && parcel build parcel/index.html --no-cache -d parcel/dist --public-url /dist/"
  },
